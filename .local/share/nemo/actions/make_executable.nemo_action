[Nemo Action]
Name=Make _Executable (Admin)
Comment=Mark %F as executable elevating privileges if necessary.
Exec=<action_scripts/make_executable.sh %F>
Icon-Name=application-x-executable-symbolic
Selection=notnone
Extensions=nodirs;
Conditions=exec <action_scripts/check_executable.sh false %F>;
EscapeSpaces=true
Dependencies=zenity;
